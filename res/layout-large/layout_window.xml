<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/TableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
	 <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/lblWinPlace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/place"
            android:layout_marginRight="8dp"
            android:textSize="20sp" />
        
        <TextView
            android:id="@+id/tvWinPlace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/lblWinCoordinate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/coordinate"
            android:layout_marginRight="8dp"
            android:textSize="16sp" />
        
        <TextView
            android:id="@+id/tvWinCoordinate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp" />
    </TableRow>
    
    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/lblWinMagnitude"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/magnitude"
            android:layout_marginRight="8dp"
            android:textSize="16sp" />
        
        <TextView
            android:id="@+id/tvWinMagnitude"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp" />

    </TableRow>
    
    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/lblWinDepth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/depth"
            android:layout_marginRight="8dp"
            android:textSize="16sp" />
        
        <TextView
            android:id="@+id/tvWinDepth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/lblWinTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/time"
            android:layout_marginRight="8dp"
            android:textSize="16sp" />
        <TextView
            android:id="@+id/tvWinTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16sp" />

    </TableRow>
    
    
</TableLayout>
